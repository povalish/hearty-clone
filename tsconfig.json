{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "screens": ["./screens"],
      "ui-kit": ["./ui-kit"],
      "assets": ["../assets"],
      "utils": ["./src/utils"],
      "navigation": ["./src/navigation"],
      "modules": ["./src/modules"]
    },
  }
}
